# udev rules for wacom devices
# will create:
#	/dev/input/wacomX
#	/dev/input/wacom-tablets/<model>-tabletX
#	/dev/input/wacom-tablets/<model>-<unique PCI/USB path>
#


ACTION!="add|change", GOTO="wacom_end"
KERNEL!="event*", GOTO="wacom_end"
ENV{ID_VENDOR_ID}!="056a", GOTO="wacom_end"

ENV{WACOM_TYPE}!="touch", SYMLINK+="input/wacom"
ENV{WACOM_TYPE}=="touch", SYMLINK+="input/wacom-touch"
ENV{ID_MODEL_ID}=="0000", SYMLINK+="input/wacom-tablets/penpartner input/wacom-tablets/penpartner-%b"
